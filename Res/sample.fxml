<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rs.tut4.Controller">
<TabPane tabClosingPolicy="UNAVAILABLE">
  <tabs>
    <Tab text="Artikli">
      <content>
        <AnchorPane>
               <children>
                  <TextArea fx:id="ulaz" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="278.0" />
                  <TextArea fx:id="izlaz" editable="false" layoutX="14.0" layoutY="225.0" prefHeight="200.0" prefWidth="279.0" />
                  <Button layoutX="309.0" layoutY="116.0" mnemonicParsing="false" onAction="#DodajArtikle" prefHeight="31.0" prefWidth="193.0" text="Dodaj artikle" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></AnchorPane>
      </content>
    </Tab>
    <Tab text="Račun">
      <content>
        <AnchorPane prefHeight="400.0" prefWidth="500.0">
               <children>
                  <ChoiceBox fx:id="artikalChoiceBox" layoutX="165.0" layoutY="46.0" prefWidth="150.0" />
                  <Label layoutX="45.0" layoutY="50.0" text="Artikal" />
                  <Spinner fx:id="spinner" cache="true" editable="true" layoutX="166.0" layoutY="125.0" prefHeight="25.0" prefWidth="151.0" />
                  <Label layoutX="45.0" layoutY="129.0" text="Količina" />
                  <Button layoutX="165.0" layoutY="173.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="87.0" text="Dodaj" />
                  <TextArea fx:id="racun" layoutX="166.0" layoutY="222.0" prefHeight="200.0" prefWidth="189.0" />
                  <Label layoutX="45.0" layoutY="293.0" text="Aktuelni račun" />
               </children></AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
        </GridPane>
